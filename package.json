{
  "name": "photounikalizer",
  "version": "2.3.1",
  "private": true,
  "main": "electron/main.js",
  "author": "YALOKGAR <yalokgar@gmail.com>",
  "description": "PhotoUnikalizer â€“ unique and anonymize photos with smart drifts and rich fake EXIF/IPTC.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YALOKGARua/PhotoUnikalizer.git"
  },
  "homepage": "https://github.com/YALOKGARua/PhotoUnikalizer#readme",
  "bugs": {
    "url": "https://github.com/YALOKGARua/PhotoUnikalizer/issues"
  },
  "keywords": [
    "photo",
    "anonymize",
    "exif",
    "iptc",
    "metadata",
    "electron",
    "react",
    "windows"
  ],
  "scripts": {
    "wasm:build": "asc wasm/assembly/index.ts -b public/wasm/pu.wasm -t public/wasm/pu.wat --optimize --noAssert",
    "wasm:watch": "npm run wasm:build -- --watch",
    "dev": "npm run native:build && concurrently -k \"vite\" \"wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "build": "vite build",
    "start": "electron .",
    "chat:server": "node server/chat-server.js",
    "chat:build": "esbuild server/chat-server.ts --platform=node --format=cjs --target=node18 --bundle --outfile=server/chat-server.js",
    "dist": "npm run build && npm run native:build && electron-builder",
    "publish": "npm run build && npm run native:build && electron-builder --publish always",
    "release:patch": "npm version patch -m \"release: %s\" && git push",
    "release:minor": "npm version minor -m \"release: %s\" && git push",
    "release:major": "npm version major -m \"release: %s\" && git push",
    "postinstall": "electron-builder install-app-deps && node scripts/build-native.js && node scripts/build-electron.js",
    "native:build": "node scripts/build-native.js",
    "electron:build": "node scripts/build-electron.js",
    "native:prebuild": "cd native && prebuildify --napi --strip",
    "assets:icons": "node scripts/make-icons.js",
    "open:sendto": "powershell -NoProfile -Command \"Start-Process explorer.exe ($env:APPDATA+'\\Microsoft\\Windows\\SendTo')\""
  },
  "dependencies": {
    "@formkit/auto-animate": "0.8.4",
    "@headlessui/react": "2.2.7",
    "@iconify/react": "^6.0.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@tippyjs/react": "4.2.6",
    "clsx": "2.1.1",
    "cmdk": "^1.1.1",
    "dotenv": "^16.4.5",
    "electron-updater": "^6.1.8",
    "framer-motion": "12.23.12",
    "i18next": "^23.16.8",
    "lucide-react": "0.542.0",
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.3",
    "sharp": "^0.33.3",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "tippy.js": "6.3.7",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react": "^4.3.0",
    "asc": "npm:assemblyscript@^0.27.27",
    "assemblyscript": "^0.27.27",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^33.2.1",
    "electron-builder": "^24.13.3",
    "esbuild": "^0.25.9",
    "node-gyp": "^10.2.0",
    "png-to-ico": "^2.1.8",
    "postcss": "^8.4.38",
    "prebuildify": "^6.0.1",
    "react-responsive": "^10.0.1",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "vite": "^5.2.0",
    "wait-on": "^7.0.1"
  },
  "overrides": {
    "wrappy": "1.0.2",
    "once": {
      "wrappy": "1.0.2"
    },
    "inflight": {
      "wrappy": "1.0.2"
    }
  },
  "build": {
    "appId": "com.yalokgar.photounikalizer",
    "productName": "PhotoUnikalizer",
    "artifactName": "PhotoUnikalizer-${version}-${os}-${arch}.${ext}",
    "copyright": "Copyright (C) 2025 YALOKGAR",
    "files": [
      "dist/**",
      "electron/**",
      "node_modules/**",
      "native/prebuilds/**",
      "native/build/Release/*.node",
      "CHANGELOG.md",
      "README.md",
      "package.json",
      "public/wasm/**",
      "!**/.vscode/**",
      "!**/.github/**",
      "!**/test/**"
    ],
    "compression": "maximum",
    "asar": true,
    "asarUnpack": [
      "node_modules/sharp/**/*",
      "native/build/Release/*.node",
      "native/prebuilds/**"
    ],
    "directories": {
      "buildResources": "build"
    },
    "win": {
      "executableName": "PhotoUnikalizer",
      "icon": "build/icon.ico",
      "requestedExecutionLevel": "requireAdministrator",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "artifactName": "PhotoUnikalizer-Setup-${version}.exe",
      "include": "build/installer.nsh"
    },
    "portable": {
      "artifactName": "PhotoUnikalizer-Portable-${version}.exe"
    }
  }
}
